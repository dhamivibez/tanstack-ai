{
  "compilerOptions": {
    "jsx": "preserve",
    "jsxFactory": "solid",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["eslint.config.js", "prettier.config.js", "src"]
}
